    "from tracker import center_of_mass\n",
    "import numpy as np\n",
    "import cv2\n",
    "\n",
    "image = np.zeros((100, 100))\n",
    "\n",
    "image[:11, :11] = 1\n",
    "\n",
    "m = cv2.moments(image)\n",
    "\n",
    "c0 = m[\"m00\"]\n",
    "cx = m[\"m10\"]\n",
    "cy = m[\"m01\"]\n",
    "\n",
    "print(f\"{cx/c0=:.2f}, {cy/c0=:.2f}\")"
   ]
